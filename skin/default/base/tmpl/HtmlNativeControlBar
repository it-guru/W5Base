<script langauge="JavaScript">
function  HtmlNativeControlBarReload(){
   document.location.href=document.location.href;
}
function  HtmlNativeControlBarPrint(){
   window.print();
}
</script>
<style>
.HtmlNativeControlBarIcon{
cursor:pointer;
padding:2px;
